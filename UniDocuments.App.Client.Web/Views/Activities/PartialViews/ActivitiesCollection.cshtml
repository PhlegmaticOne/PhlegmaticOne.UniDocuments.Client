@model IList<ActivityDisplayObject>

<div class="card-group vgr-cards mt-2 justify-content-around">
    @foreach (var activityDisplayObject in Model)
    {
        <div>
            @await Html.PartialAsync("~/Views/Activities/PartialViews/ActivityItem.cshtml", activityDisplayObject, ViewData)
        </div>
    }
</div>