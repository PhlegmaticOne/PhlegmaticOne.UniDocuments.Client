@model List<UniDocuments.App.Shared.Activities.Detailed.ActivityDetailedStudentObject>

@if (Model.Count == 0)
{
    <div class="row mt-3">
        <p class="text-white text-center">
            В активности нет ни одного сутдента
        </p>
    </div>
}
else
{
    <ul class="list list-inline">
        @foreach (var student in Model)
        {
            @await Html.PartialAsync("~/Views/Activities/PartialViews/ActivityDetailedStudent.cshtml", student);
        }
    </ul>
}